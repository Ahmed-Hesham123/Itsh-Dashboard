                                    img(src="imgs/activity-02.png" alt="Store")
                                    .info
                                        span.d-block.mb-10 Academy
                                        span.c-grey Got The PHP Certificate
                                    .date
                                        span.d-block.mb-10 16:05
                                        span.c-grey Yesterday
                                .activity.d-flex.align-center.txt-c-mobile
                                    img(src="imgs/activity-03.png" alt="Store")
                                    .info
                                        span.d-block.mb-10 Badges
                                        span.c-grey Unlocked The 10 Skills Badge
                                    .date
                                        span.d-block.mb-10 18:05
                                        span.c-grey Yesterday
                                .activity.d-flex.align-center.txt-c-mobile
                                    img(src="imgs/activity-01.png" alt="Store")
                                    .info
                                        span.d-block.mb-10 Store
                                        span.c-grey Bought The Typescript Course
                                    .date
                                        span.d-block.mb-10 12:05
                                        span.c-grey Yesterday
                            <!-- End Activities -->
                        <!-- End Other Data -->
